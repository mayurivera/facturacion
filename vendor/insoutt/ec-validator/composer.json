{
    "name": "insoutt/ec-validator",
    "description": "Validador de Cedula, RUC y mas datos de Ecuador",
    "keywords": [
        "insoutt",
        "ec-validator"
    ],
    "homepage": "https://github.com/insoutt/ec-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "insoutt",
            "email": "hi@elvisfernando.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.3.2",
        "friendsofphp/php-cs-fixer": "^3.21.1",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Insoutt\\EcValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Insoutt\\EcValidator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
